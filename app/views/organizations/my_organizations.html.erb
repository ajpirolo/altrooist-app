<h1>My Organizations</h1>
<% @my_organizations.each do |mo| %>
  <p><%= image_tag(mo.photo_url) %></p>
  <p><%= mo.name %></p>
  <p><%= mo.address %></p>
  <p><%= mo.phone %></p>
  <p><%= mo.email %></p>
  <p><%=  link_to(new_organization_journal_entry_path(mo)) do %></p>
      <%= image_tag("pencil.png") %>
    <% end %>
<% end %>
